@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_epic_#_epic_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxepic.html> ;
        modgui:stylesheet <modgui/stylesheet-gxepic.css> ;
        modgui:screenshot <modgui/screenshot-gxepic.png> ;
        modgui:thumbnail <modgui/thumbnail-gxepic.png> ;
        modgui:brand " Guitarix " ;
        modgui:label "GxEpic" ;
        modgui:model "panel" ;
        modgui:panel "0720" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "GAIN" ;
            lv2:name "GAIN" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "TONE" ;
            lv2:name "TONE" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "VOLUME" ;
            lv2:name "VOLUME" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "BOOST" ;
            lv2:name "BOOST" ;
        ] ;
    ] .
